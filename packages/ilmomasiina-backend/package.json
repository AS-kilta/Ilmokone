{
  "name": "@tietokilta/ilmomasiina-backend",
  "version": "2.0.0-alpha10",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tietokilta/ilmomasiina.git"
  },
  "scripts": {
    "start": "bnr start:dev",
    "start:dev": "bnr start:dev",
    "start:prod": "bnr start:prod",
    "stop:prod": "pm2 stop 0",
    "monit:prod": "pm2 monit",
    "build": "tsc --build",
    "clean": "rimraf dist",
    "typecheck": "tsc --build"
  },
  "betterScripts": {
    "start:dev": {
      "command": "ts-node-dev -r tsconfig-paths/register --respawn --project tsconfig.json src/bin/server.ts",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": "3001"
      }
    },
    "start:prod": {
      "command": "pm2 start dist/bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.15",
    "@feathersjs/authentication-local": "^4.5.15",
    "@feathersjs/errors": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@tietokilta/ilmomasiina-models": "workspace:2.0.0-alpha10",
    "@types/cors": "^2.8.12",
    "base32-encode": "^1.2.0",
    "better-npm-run": "^0.1.1",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "email-templates": "^8.1.0",
    "express": "^4.18.1",
    "express-sslify": "^1.2.0",
    "feathers-hooks-common": "^5.0.6",
    "feathers-sequelize": "^6.3.4",
    "ics": "^2.35.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.1",
    "nodemailer": "^6.7.5",
    "nodemailer-mailgun-transport": "^2.1.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.0",
    "umzug": "^3.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/debug": "^4.1.7",
    "@types/email-templates": "^8.0.4",
    "@types/express": "^4.17.13",
    "@types/express-sslify": "^1.2.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.41",
    "@types/node-cron": "^3.0.1",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "~4.3.5"
  }
}
