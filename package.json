{
  "name": "ilmomasiina",
  "main": "index.js",
  "engines": {
    "node": "^8.9.4",
    "npm": "^5.6.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin config src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "monit:prod": "better-npm-run monit:prod",
    "stop:prod": "better-npm-run stop:prod",
    "dev": "better-npm-run dev",
    "test": "npm run lint && better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "reset-db": "better-npm-run reset-db",
    "create-fake-data": "better-npm-run create-fake-data",
    "create-user": "better-npm-run create-user",
    "test-email": "better-npm-run test-email"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "reset-db": {
      "command": "node scripts/reset-database",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "create-fake-data": {
      "command": "node scripts/create-fake-data",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "create-user": {
      "command": "node scripts/create-user",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test-email": {
      "command": "node scripts/test-email",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "pm2 start ./bin/prod-server.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "stop:prod": {
      "command": "pm2 stop 0"
    },
    "monit:prod": {
      "command": "pm2 monit"
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/athenekilta/ilmomasiina"
  },
