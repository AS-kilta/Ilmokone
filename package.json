{
  "name": "react-redux-starter-kit",
  "version": "3.0.0-alpha.2",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": "^12.20.1",
    "npm": "^6.14.13"
  },
  "scripts": {
    "clean": "bnr clean",
    "compile:client": "bnr compile:client",
    "compile:server": "bnr compile:server",
    "deploy": "bnr deploy",
    "deploy:dev": "bnr deploy:dev",
    "deploy:prod": "bnr deploy:prod",
    "start": "bnr start:dev",
    "dev": "bnr dev",
    "start:dev": "bnr start:dev",
    "start:prod": "bnr start:prod",
    "monit:prod": "bnr monit:prod",
    "stop:prod": "bnr stop:prod",
    "lint": "bnr lint",
    "lint:fix": "bnr lint:fix",
    "test": "bnr lint && bnr test",
    "test:dev": "npm run test -- --watch",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "reset-db": "bnr reset-db",
    "create-fake-data": "bnr create-fake-data",
    "create-user": "bnr create-user",
    "test-email": "bnr test-email"
  },
  "directories": {
    "lib": "server"
  },
  "betterScripts": {
    "compile:client": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile:server": {
      "command": "tsc -p server"
    },
    "dev": {
      "command": "nodemon server/dist/bin/dev-server --ignore dist --ignore server/dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "reset-db": {
      "command": "node server/scripts/reset-database",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "create-fake-data": {
      "command": "node server/scripts/create-fake-data",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "create-user": {
      "command": "node server/scripts/create-user",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test-email": {
      "command": "node server/scripts/test-email",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile:server && npm run compile:client",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start:dev": {
      "command": "ts-node-dev --respawn --project server/tsconfig.json server/bin/dev-server.ts",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "npm run compile:server && pm2 start ./server/dist/bin/prod-server.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "stop:prod": {
      "command": "pm2 stop 0"
    },
    "monit:prod": {
      "command": "pm2 monit"
    },
    "test": {
      "command": "npm run compile:server && node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "lint": {
      "command": "eslint bin config server src tests"
    },
    "lint:fix": {
      "command": "npm run lint -- --fix"
    },
    "clean": {
      "command": "rimraf dist server/dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "@feathersjs/adapter-commons": "^4.5.11",
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@sendgrid/mail": "^6.3.1",
    "@types/babel-core": "^6.17.0",
    "@types/compression": "^1.7.0",
    "@types/debug": "^4.1.5",
    "@types/email-templates": "^8.0.2",
    "@types/express-sslify": "^1.2.1",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",
    "@types/node": "^14.14.27",
    "@types/node-cron": "^2.0.3",
    "@types/node-sass": "^4.11.1",
    "@types/validator": "^13.1.3",
    "antd": "^3.8.2",
